#set($header_params={"css":"jibinfoFinance/mobile/css/mat_plugins.css,jibinfoFinance/mobile/css/appointment.css","Keywords":"坚卓-金融车","Description":"坚卓-金融车","title":"坚卓-金融车"})
#frontend_header($header_params)
<div class="page">
	<div class="first_div">
    	<div class="header-base-color sq-index-nav ">
            <header class="bar bar-nav header-bg">
                <a href="javascript:history.back();" class="back-to-prev-btn"><span class="icon icon-left"></span></a>
                <h2 class="text-center bar-title">经销商详情</h2>
            </header>
        </div>
        <div class="disDescrContent">
            <div class="company">
    			<p id="dealerName" class="companyName">江苏安吉汽车销售服务有限公司
    				<img src="$static_path/jibinfoFinance/mobile/images/none_medal.png" alt=""></p>
    			<div>
        			<span class="apply_num"><span>1354</span>人预约</span>
        			<span class="apply_marker">神速贷</span>
                </div>
    			<img id="brandUrl" src="$static_path/jibinfoFinance/mobile/images/dazhong.png" alt="">
    		</div>
    		<div class="address">
    			<p id="address">南京市卡子门大街78号</p>
    			<div class="address_icon"></div>
    			<span class="icon icon-right"></span>
    		</div>
    		<div class="btnContrain">
    			<button class="emphasized">预约办理</button>
    		</div>
    	</div>
        <div class="comment_content">
            <p class="title">用户评价(<span>112</span>)</p>
            <ul>
    			<li>
    				<div class="user">
    					<div class="userImg"><img src="$static_path/jibinfoFinance/mobile/images/user.png"/></div>
    					<span>匿名</span>
    				</div>
    				<div class="user-right">
                        <span class="utilities-profile-title">优酷会员（月卡）20元 （卡密充值）</span>
                        <div class="utilities-star">
                            <div class="rating rating_div" style="width: 20%;"></div>
                        </div>
    					<p class="date">04-15</p>
                    </div>
                    <div class="clear"></div>
    			</li>
    		</ul>
    		<div class="moreComment">更多评价</div>
    	</div>
    </div>
    <div class="dealer_content" style="display: none;">
       <div class="header-base-color sq-index-nav ">
            <header class="bar bar-nav header-bg">
                <a href="javascript:;" class="back-to-prev-btn closeDialog"><span class="icon icon-left"></span></a>
                <h2 class="text-center bar-title">预约</h2>
            </header>
        </div>
        <div>
             <div class="item-content">
                 <div class="item-inner">
                      <div class="item-title label">关注车型</div>
                      <div class="item-input">
						  <input type="text" id="carBrandName" placeholder="" readonly>
              		  </div>
					  <span class="icon icon-right selectSubBrand"></span>
				  </div>
			 </div>
			 <div class="item-content">
				 <div class="item-inner">
					  <textarea id="remark" type="text" placeholder="输入关注车型情况或贷款咨询等您想了解的信息"></textarea>
				  </div>
             </div>
			 <div class="item-content  margin-to">
    			 <div class="item-inner">
                          <div class="item-title label">手机号</div>
                          <div class="item-input">
    						  <input type="text" id="mobile" placeholder="请输入您的手机号">
                  		  </div>
    				  </div>
    			 </div>
             </div>
			 
			 <div class="btnContrain margin-to">
    			 <button class="emphasized">提交</button>
    		</div>
		</div>
	</div>
	<section class="mat-right J_silder_subBrands">
        <div class="mat-slideWarp">
            <div class="mat-slidePannel">
                <!-- 后端刷入的内容 -->
                <div class="header-base-color">
                    <header class="bar bar-nav header-bg">
                        <a href="javascript:;" class="back-to-prev-btn" id="brandselectDia"><span class="icon icon-left"></span></a>
                        <h2 class="text-center bar-title">城市选择</h2>
                    </header>
                </div>
                <aside class="m-aside">
                    <ul class="J_subBrands">
						
                    </ul>
                </aside>
            </div>
        </div>
    </section>
</div>
<script type="text/template" id="subBrandsTemplate">
    <li  data-code="{code}"><a href="javascript:;">{name}</a></li>
</script>
#set($footerParams={"js":"jibinfoFinance/mobile/scripts/description.js"})
#frontend_footer($footerParams)